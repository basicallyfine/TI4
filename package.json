{
    "name": "ti4-reference",
    "version": "0.0.1",
    "private": true,
    "homepage": "/ti4-reference/",
    "dependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "bootstrap": "^4.5.0",
        "history": "4.9.0",
        "lodash": "^4.17.15",
        "node-sass": "^4.14.1",
        "object-hash": "^2.0.3",
        "querystring": "^0.2.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-icons": "^3.11.0",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "3.4.1",
        "swiper": "^6.1.2"
    },
    "scripts": {
        "build-css": "node-sass src/ -o src/",
        "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
        "start-js": "PORT=3001 react-app-rewired start",
        "start": "npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-app-rewired build",
        "deploy": "git subtree push --prefix build origin gh-pages",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "npm-run-all": "^4.1.5",
        "react-app-rewire-yaml": "^1.1.0",
        "react-app-rewired": "^2.1.6",
        "yaml-loader": "^0.6.0"
    }
}
